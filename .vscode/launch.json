{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "start dev",
      "command": "yarn start:dev",
      "request": "launch",
      "type": "node-terminal"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Launch",
      "cwd": "${workspaceRoot}",
      "runtimeArgs": ["-r", "ts-node/register"],
      "args": ["${workspaceRoot}/index.ts"]
    }
  ]
}
